{
  "type": "project",
  "require": {
    "php": "^7.4"
  },
  "autoload": {
    "psr-4": {
      "App\\":"src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "scripts": {
    "phpunit": "./vendor/bin/phpunit",
    "test": [
      "@phpunit"
    ],
    "app": [
      "@test",
      "php ./public/index.php",
      "php ./public/index_duo.php",
      "php ./public/index_pro.php"
    ]
  }
}